<i:EventTrigger x:Class="RoxorBot.Data.ViewHelpers.EventToCommand"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
             x:Name="CustomEventTrigger">
    <i:InvokeCommandAction Command="{Binding Command, ElementName=CustomEventTrigger}" />
</i:EventTrigger>
